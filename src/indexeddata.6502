\\ Miscellaneous tables and data accessed via indexed mode
\\ which we don't want to cross page boundaries


\\ Used by the IRQ handler

.bottompalette
	EQUB &22, &81, &A4		; magenta, cyan, yellow

.toppalette
	EQUB &23, &86, &A5		; blue, red, green


\\ Used by line renderer / timer bars

.leftmask
	EQUB &FF,&77,&33,&11
.rightmask
	EQUB &00,&88,&CC,&EE
.pixelmask
	EQUB &88,&44,&22,&11


\\ Used by distancealongray

.dartablelo
	EQUB LO(distancealongray0), LO(distancealongray1), LO(distancealongray2_stub), LO(distancealongray3_stub)


\\ Colour tables

colour_left_black  = %00000000
colour_left_blue   = %00001010
colour_left_red    = %10100000
colour_left_green  = %10101010
colour_right_black = %00000000
colour_right_blue  = %00000101
colour_right_red   = %01010000
colour_right_green = %01010101

.colourtab0
	EQUB colour_left_black + colour_right_black		: colour_black = 0
	EQUB colour_left_blue  + colour_right_black		: colour_deep_blue = 1
	EQUB colour_left_blue  + colour_right_black		: colour_dark_blue = 2
	EQUB colour_left_blue  + colour_right_black		: colour_medium_blue = 3
	EQUB colour_left_blue  + colour_right_blue		: colour_bright_blue = 4
	EQUB colour_left_red   + colour_right_black		: colour_deep_red = 5
	EQUB colour_left_red   + colour_right_black		: colour_dark_red = 6
	EQUB colour_left_red   + colour_right_black		: colour_medium_red = 7
	EQUB colour_left_red   + colour_right_red		: colour_bright_red = 8
	EQUB colour_left_green + colour_right_black		: colour_deep_green = 9
	EQUB colour_left_green + colour_right_black		: colour_dark_green = 10
	EQUB colour_left_green + colour_right_black		: colour_medium_green = 11
	EQUB colour_left_green + colour_right_green		: colour_bright_green = 12
	EQUB colour_left_blue  + colour_right_red		: colour_violet = 13
	EQUB colour_left_blue  + colour_right_red		: colour_purple = 14
	EQUB colour_left_blue  + colour_right_red		: colour_fuchsia = 15
	EQUB colour_left_blue  + colour_right_green		: colour_teal = 16
	EQUB colour_left_blue  + colour_right_green		: colour_sky_blue = 17
	EQUB colour_left_blue  + colour_right_green		: colour_turquoise = 18
	EQUB colour_left_red   + colour_right_green		: colour_orange= 19
	EQUB colour_left_red   + colour_right_green		: colour_olive = 20
	EQUB colour_left_red   + colour_right_green		: colour_autumn_green = 21
	
.colourtab1
	EQUB colour_left_black + colour_right_black		; colour_black
	EQUB colour_left_black + colour_right_black		; colour_deep_blue
	EQUB colour_left_black + colour_right_blue		; colour_dark_blue
	EQUB colour_left_blue  + colour_right_blue		; colour_medium_blue
	EQUB colour_left_blue  + colour_right_blue		; colour_blue
	EQUB colour_left_black + colour_right_black		; colour_deep_red
	EQUB colour_left_black + colour_right_red		; colour_dark_red
	EQUB colour_left_red   + colour_right_red		; colour_medium_red
	EQUB colour_left_red   + colour_right_red		; colour_red
	EQUB colour_left_black + colour_right_black		; colour_deep_green
	EQUB colour_left_black + colour_right_green		; colour_dark_green
	EQUB colour_left_green + colour_right_green		; colour_medium_green
	EQUB colour_left_green + colour_right_green		; colour_bright_green
	EQUB colour_left_blue  + colour_right_blue		; colour_violet
	EQUB colour_left_red   + colour_right_blue		; colour_purple
	EQUB colour_left_red   + colour_right_red		; colour_fuchsia
	EQUB colour_left_blue  + colour_right_blue		; colour_teal
	EQUB colour_left_green + colour_right_blue		; colour_sky_blue
	EQUB colour_left_green + colour_right_green		; colour_turquoise
	EQUB colour_left_red   + colour_right_red		; colour_orange
	EQUB colour_left_green + colour_right_red		; colour_olive
	EQUB colour_left_green + colour_right_green		; colour_autumn_green


\\ Used by timer bars

bar_magenta = &0F
bar_cyan = &F0
bar_yellow = &FF

.barcol1
	EQUB bar_magenta
	EQUB (bar_magenta AND &AA) OR (bar_cyan AND &55)
	EQUB bar_cyan
	EQUB (bar_yellow AND &AA) OR (bar_magenta AND &55)
	EQUB bar_yellow
	EQUB (bar_cyan AND &AA) OR (bar_yellow AND &55)
	EQUB bar_magenta
.barcol2
	EQUB bar_magenta
	EQUB (bar_cyan AND &AA) OR (bar_magenta AND &55)
	EQUB bar_cyan
	EQUB (bar_magenta AND &AA) OR (bar_yellow AND &55)
	EQUB bar_yellow
	EQUB (bar_yellow AND &AA) OR (bar_cyan AND &55)
	EQUB bar_magenta


IF P% > codestart+&100
	ERROR "Tables overran page"
ENDIF
	

ALIGN &100
